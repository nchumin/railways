<tr>
  <td><%= ticket.train.number %></td>
  <td><%= ticket.start_station.name %></td>
  <td><%= ticket.end_station.name %></td>
  <td><%= ticket.passenger_name %></td>
  <td><%= ticket.user.email %></td>
  <td><%= link_to t('common.show'), [:admin, ticket] %></td>
  <td><%= link_to t('common.edit'), edit_admin_ticket_path(ticket) %></td>
  <td><%= link_to t('common.delete'), [:admin, ticket], method: :delete, data: { confirm: t('common.sure') } %></td>
</tr>